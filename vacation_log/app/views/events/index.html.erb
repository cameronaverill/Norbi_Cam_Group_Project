<h1> Our Vacation Log </h1>

<% @events.each do |event|%>
<p>
	<tr> 
		<td> <%= link_to event.name, event_path(event)%> </td>
		<td> <%= link_to 'Edit', edit_event_path(event) %> </td>
		<td> <%= link_to 'Delete', event_path(event), method: :delete, data: { confirm: 'Are you sure?' } %> </td>
	</tr>
	</p>
<%end%>